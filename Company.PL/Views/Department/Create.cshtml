@model CreateDepartmentDto

    
@{
    ViewData["Title"] = "Create";
}

<h1>Create Department</h1>

<br />
<br />
<br />
<form asp-action="Create">
    <div class="m-3">
    <label asp-for="code" class="col-form-label">Code</label>
    <input asp-for="code" class="form-control"/>
        <span asp-validation-for="code" class="text-danger"></span>
    </div>
    
    <div class="m-3">
        <label asp-for="name" class="col-form-label"></label>
        <input asp-for="name" class="form-control"/>
        <span asp-validation-for="name" class="text-danger"></span>

    </div>

    <div class ="m-3">
        <label asp-for="CreateAt" class="col-form-label"></label>
        <input asp-for="CreateAt" class="form-control" />
        <span asp-validation-for="CreateAt" class="text-danger"></span>

    </div>

    <div class="m-3">
        <input type="submit"  value="Create" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-light">Back to list </a>
    </div>





</form>
